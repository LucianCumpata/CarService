        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.461,0.353">
        <Button x:Name="Add_client" Content="Add client" HorizontalAlignment="Left" Margin="518,334,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.4,2.5" Click="Button_Click"/>
